{
  "name": "applied-mindfulness-platform",
  "version": "1.0.0",
  "description": "Comprehensive mindfulness application platform with AI coaching and community features",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server/index.js",
    "client:dev": "cd src/client && npm run dev",
    "build": "npm run client:build && npm run server:build",
    "client:build": "cd src/client && npm run build",
    "server:build": "echo 'Server build placeholder'",
    "start": "node src/server/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "db:migrate": "node src/server/scripts/migrate.js",
    "db:seed": "node src/server/scripts/seed.js"
  },
  "keywords": ["mindfulness", "meditation", "ai-coaching", "wellness", "community"],
  "author": "Applied Mindfulness Training",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "aws-sdk": "^2.1498.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}